            "App\\Services\\": "app/src/Services",
            "App\\Auth\\": "app/src/Auth"
        }
    },
    "require": {
        "slim/slim": "^3.0",
        "slim/twig-view": "^2.0",
        "slim/flash": "^0.1.0",
        "monolog/monolog": "^1.13",
        "illuminate/database": "~5.1",
        "nanaksr/validator": "^1.2"
    }
